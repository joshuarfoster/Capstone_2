{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "creatorUsername"
    ],
    "additionalProperties": false,
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Course1"
            ]
        },
        "creatorUsername": {
            "type": "string",
            "default": "",
            "title": "The creatorUsername Schema",
            "examples": [
                "user"
            ]
        },
        "about": {
            "type": "string",
            "default": "",
            "title": "The about Schema",
            "examples": [
                "description"
            ]
        }
    },
    "examples": [{
        "title": "Course1",
        "creatorUsername": "user",
        "about": "description"
    }]
}